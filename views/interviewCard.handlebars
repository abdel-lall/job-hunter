
<div class="interviewCard" data-id={{data.id}}>
    <a href={{data.url}} class="IpositinTitle" target="_blank">{{data.title}}</a>
    <div class="IcompanyName"><i class="far fa-building buidingIcon"></i>{{data.employer}}</div>
    <div class="IpositionLocation"><i class="fas fa-map-marker-alt mapmarkerIcon"></i>{{data.location}}
    </div>
    <div class="IpositionDescription">
        {{data.description}}
    </div>
    <div class="IinterviewTime">
        <input type="datetime-local" class="meeting-time" name="meeting-time" value={{data.reminder}}>
        <button class="editDateTime" data-id={{data.id}}><i class="editDTIcon far fa-edit" data-id={{data.id}}></i></button>
    </div>
    <div class="IdeleteNextDiv">
        <button class="Idelete" data-id={{data.id}}><i class="far fa-trash-alt"></i></button>
        <button class="Inext" data-id={{data.id}}><i class="fas fa-arrow-right"></i></button>
    </div>
</div>