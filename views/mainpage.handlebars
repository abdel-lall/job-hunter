<head>
    <meta charset="utf-8">

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.3/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
        integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link href="//netdna.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
    <link rel="stylesheet" href="/styles/mainpage.css">
    <title>Dashboard</title>
</head>

<body>
    <nav class="navbar " id="navbar">
        <a class="navbar-brand" href="#">
            <img src="/images/jobhunter2.png" width="30" height="30" class="d-inline-block align-top" alt="">
            <span id="titlejob">Job</span><span id="titlehunter">Hunter</span></a>
        </a>

        <form class="form-inline">
            <div class="btn-group">
                <button type="button" class="btn btn-secondary dropdown-toggle" data-toggle="dropdown"
                    aria-haspopup="true" aria-expanded="false" id="navbardropdown">
                    <img src={{user.image}} alt="profilepicture" id="navbarprofilepicture">
                </button>
                <div class="dropdown-menu dropdown-menu-right" id="dropdownmenu">

                    <div id="dropdownprofileinfo">

                        <div id="dropdownprofileimagediv">
                            <img src={{user.image}} alt="profileimage" id="dropdownprofileimage">
                        </div>
                        <div id="dropdownprofileinfodiv">
                            <h1 id="dropdownprofileusername">{{user.name}}</h1>
                            <p id="dropdownprofileemail">{{user.email}}</p>
                        </div>

                    </div>

                    <button class="dropdown-item" type="button" onclick="window.location.href = '/mainpage'"><i
                            class="fas fa-home"></i> Home</button>
                    <button class="dropdown-item" type="button" id="huntingtools"><i class="fas fa-briefcase"></i>
                        Hunting tools</button>
                    <button class="dropdown-item" type="button" id="settings"><i class="fas fa-cog"></i>
                        Settings</button>
                    <button class="dropdown-item" type="button" id="signout"
                        onclick="window.location.href = '/logout'"><i class="fas fa-sign-out-alt"></i> Sign
                        out</button>
                </div>
            </div>
        </form>
    </nav>
    <div class="conatiner-fluid" id="maincontainer">

        <div class="sections" id="firstsection">
            <div class="container sectioncontainer" id="firstsectioncontainer">
                <div class="row sectionnavbar" id="searchjobsnavbarrow">
                    <div class="col-9 text-left sectiontitle">
                        Job search
                    </div>
                    <div class="col-3 text-right sectionicon">
                        <button class="enlarge" data-id="firstsection"><i
                                class="fas fa-expand-alt enlargeicon"></i></button>
                    </div>
                </div>
                <div class="row sectioncontentrow" id="searchcontentrow">
                    <div class="container-fluid sectioncontentcontainer" id="searchcontentcontainer">
                        <div class="row sectioncontentelement" id="searchjobcontainer">
                            <div class="searchjomb">
                                <div class="container-fluid" id="searchcontainer">
                                    <div class="row align-items-center" id="searchinputfirstrow">
                                        <div class="col-6 inputcol">
                                            <input type="text" class="inputiconplaceholder" id="keyword"
                                                placeholder=" Keyword">
                                        </div>
                                        <div class="col-6 inputcol">
                                            <input type="text" class="inputiconplaceholder" id="location"
                                                placeholder=" Location">
                                        </div>
                                    </div>
                                    <div class="row align-items-center" id="searchinputseconderow">
                                        <div class="col-6 inputcol">
                                            <div class="input-group mb-3">
                                                <select class="custom-select" id="inputGroupSelect01">
                                                    <option class="inputoption" value="Indeed">Indeed</option>
                                                    <option class="inputoption" value="USAJobs">USAJobs</option>
                                                </select>
                                            </div>
                                        </div>
                                        <div class="col-6 inputcol text-center">
                                            <button type="button" class="btn" id="search"><i class="fas fa-search"></i>
                                                Search</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                        <p class="searchhere">Search for jobs here</p>
                    </div>
                </div>
            </div>
        </div>
        <div class="sections " id="secondesection">
            <div class="container sectioncontainer" id="secondesectioncontainer">
                <div class="row sectionnavbar" id="savedjobsnavbarrow">
                    <div class="col-9 text-left sectiontitle">
                        Saved jobs
                    </div>
                    <div class="col-3 text-right sectionicon">
                        <button class="enlarge" data-id="secondesection"><i
                                class="fas fa-expand-alt enlargeicon"></i></button>
                    </div>
                </div>
                <div class="row sectioncontentrow" id="savedcontentrow">
                    <div class="container-fluid sectioncontentcontainer" id="savedjobscontainer">
                        {{#if datasaved}}

                        {{#each datasaved}}
                        {{!-- {{#if (status == "saved")}} --}}
                        <div class="row sectioncontentelement savedjobssectioncontentelement" data-id={{id}}>
                            <div class="elements savedelements ">
                                <div class="card" data-id={{id}} data-url={{url}}>
                                    <div class="card-body">
                                        <h5 class="card-title"><a class="cardtitleanchor" href={{url}}
                                                target="_blank">{{title}}</a></h5>
                                        <p class="card-text employer">
                                            <i class="far fa-building"></i>
                                            {{organisation}}</p>
                                        <p class="card-text location">
                                            <i class="fas fa-map-marker-alt"></i>

                                            {{location}},

                                        </p>
                                        <p class="card-text showmore"><button class="showmorelocation"><i
                                                    class="fas fa-ellipsis-h"></i></button></i></p>
                                        <p class="card-text description"><i class="fas fa-tasks"></i> {{description}}
                                        </p>
                                        <p class="card-text showmore"><button class="showmoredescriton"><i
                                                    class="fas fa-ellipsis-h"></i></button></i></p>
                                        <a href={{url}} class="btn apply" target="_blank">Apply</a>
                                        <a href="#" class="btn delete" data-id={{id}}>Delete</a>

                                    </div>
                                </div>
                            </div>
                        </div>
                        {{!-- {{/if}} --}}
                        {{/each}}
                        {{else}}
                        <p class="noresaultfnound" id="nosavedjobsmsg">No saved jobs</p>
                        {{/if}}

                    </div>
                </div>
            </div>
        </div>
        <div class="sections" id="thirdsection">
            <div class="container sectioncontainer">
                <div class="row sectionnavbar" id="applicationprocessnavbarrow">
                    <div class="col-9 text-left sectiontitle">
                        Application progress
                    </div>
                    <div class="col-3 text-right sectionicon">
                        <button class="enlarge" data-id="thirdsection"><i
                                class="fas fa-expand-alt enlargeicon"></i></button>
                    </div>
                </div>
                <div class="row" id="verticalmainrow">
                    <div class="container-fluid" id="verticalcontainer">
                        <div class="verticalsections" id="firstVS">
                            <div class="container VScontainer">
                                <div class="row verticalsectionnavbar" id="applicationnavbarrow">
                                    <div class="col text-left VStitle">
                                        Applied for
                                    </div>
                                    <div class="col text-right VSicon">
                                        <button class="grow" data-id="firstVS"><i
                                                class="fas fa-expand-alt growicon"></i></button>
                                    </div>
                                </div>
                                <div class="row sectioncontentrow" id="sectioncontentrowfirst">
                                    <div class="container-fluid sectioncontentcontainer"
                                        id="applicationsectioncontentcontainer">
                                        {{#if dataapplication}}

                                        {{#each dataapplication}}
                                        {{!-- {{#if status == "applied"}} --}}
                                        <div class="row sectioncontentelement applicationsectioncontentelement"
                                            data-id={{id}}>
                                            <div class="elements applicationelements">
                                                <div class="card" data-id={{id}} data-url={{url}}>
                                                    <div class="card-body">
                                                        <h5 class="card-title"><a class="cardtitleanchor" href={{url}}
                                                                target="_blank">{{title}}</a></h5>
                                                        <p class="card-text employer">
                                                            <i class="far fa-building"></i>
                                                            {{organisation}}</p>
                                                        <p class="card-text location">
                                                            <i class="fas fa-map-marker-alt"></i>

                                                            {{location}},

                                                        </p>
                                                        <p class="card-text showmore"><button
                                                                class="showmorelocation"><i
                                                                    class="fas fa-ellipsis-h"></i></button></i></p>
                                                        <p class="card-text description"><i class="fas fa-tasks"></i>
                                                            {{description}}
                                                        </p>
                                                        <p class="card-text showmore"><button
                                                                class="showmoredescriton"><i
                                                                    class="fas fa-ellipsis-h"></i></button></i></p>

                                                        <a href="#" class="btn delete" data-id={{id}}>Delete</a>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        {{!-- {{/if}} --}}
                                        {{/each}}
                                        {{else}}
                                        <p class="noresaultfnound" id="nosapplication">No applications</p>
                                        {{/if}}
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="verticalsections" id="secondeVS">
                            <div class="container VScontainer">
                                <div class="row verticalsectionnavbar" id="interviewnavbarrow">
                                    <div class="col text-left VStitle">
                                        Wainting for the interview
                                    </div>
                                    <div class="col text-right VSicon">
                                        <button class="grow" data-id="secondeVS"><i
                                                class="fas fa-expand-alt growicon"></i></button>
                                    </div>
                                </div>
                                <div class="row sectioncontentrow" id="sectioncontentrowseconde">
                                    <div class="container-fluid sectioncontentcontainer"
                                        id="interviewsectioncontentcontainer">
                                        {{#if datainterview}}

                                        {{#each datainterview}}
                                        {{!-- {{#if status == "interview"}} --}}
                                        <div class="row sectioncontentelement enterviewsectioncontentelement"
                                            data-id={{id}}>
                                            <div class="elements interviewelements">
                                                <div class="card" data-id={{id}} data-url={{url}}>
                                                    <div class="card-body">
                                                        <h5 class="card-title"><a class="cardtitleanchor" href={{url}}
                                                                target="_blank">{{title}}</a></h5>
                                                        <p class="card-text employer">
                                                            <i class="far fa-building"></i>
                                                            {{organisation}}</p>
                                                        <p class="card-text location">
                                                            <i class="fas fa-map-marker-alt"></i>

                                                            {{location}},

                                                        </p>
                                                        <p class="card-text showmore"><button
                                                                class="showmorelocation"><i
                                                                    class="fas fa-ellipsis-h"></i></button></i></p>
                                                        <p class="card-text description"><i class="fas fa-tasks"></i>
                                                            {{description}}
                                                        </p>
                                                        <p class="card-text showmore"><button
                                                                class="showmoredescriton"><i
                                                                    class="fas fa-ellipsis-h"></i></button></i></p>

                                                        <a href="#" class="btn delete" data-id={{id}}>Delete</a>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        {{!-- {{/if}} --}}
                                        {{/each}}
                                        {{else}}
                                        <p class="noresaultfnound" id="nointerv">No interveiws scheduled</p>
                                        {{/if}}

                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="verticalsections" id="thirdVS">
                            <div class="container VScontainer" id="thirdVScontainer">
                                <div class="row verticalsectionnavbar" id="acceptencenavbarrow">
                                    <div class="col text-left VStitle">
                                        Accepted
                                    </div>
                                    <div class="col text-right VSicon">
                                        <button class="grow" data-id="thirdVS"><i
                                                class="fas fa-expand-alt growicon"></i></button>
                                    </div>
                                </div>
                                <div class="row sectioncontentrow" id="sectioncontentrowthird">
                                    <div class="container-fluid sectioncontentcontainer"
                                        id="acceptensesectioncontentcontainer">
                                        {{#if dataacceptence}}

                                        {{#each dataacceptence}}
                                        {{!-- {{#if status == "accepted"}} --}}
                                        <div class="row sectioncontentelement acceptencesectioncontentelement"
                                            data-id={{id}}>
                                            <div class="elements acceptenceelements">
                                                <div class="card" data-id={{id}} data-url={{url}}>
                                                    <div class="card-body">
                                                        <h5 class="card-title"><a class="cardtitleanchor" href={{url}}
                                                                target="_blank">{{title}}</a></h5>
                                                        <p class="card-text employer">
                                                            <i class="far fa-building"></i>
                                                            {{organisation}}</p>
                                                        <p class="card-text location">
                                                            <i class="fas fa-map-marker-alt"></i>

                                                            {{location}},

                                                        </p>
                                                        <p class="card-text showmore"><button
                                                                class="showmorelocation"><i
                                                                    class="fas fa-ellipsis-h"></i></button></i></p>
                                                        <p class="card-text description"><i class="fas fa-tasks"></i>
                                                            {{description}}
                                                        </p>
                                                        <p class="card-text showmore"><button
                                                                class="showmoredescriton"><i
                                                                    class="fas fa-ellipsis-h"></i></button></i></p>
                                                        <a href="#" class="btn delete" data-id={{id}}>Delete</a>

                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        {{!-- {{/if}} --}}
                                        {{/each}}
                                        {{else}}
                                        <p class="noresaultfnound" id="noacceptence">have not been accepted</p>
                                        {{/if}}


                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

    </div>
    <div class="modal" tabindex="-1" role="dialog" id="modalsettings">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Settings</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="settingslines">
                        <h1 class="settingstitles">Profile picture:</h1>
                    </div>
                    <div class="settingslines">
                        <img src={{user.image}} alt="profileimage" id="settingsprofileimage">
                    </div>
                    <div class="settingslines">
                          <form class="uploadImage" enctype="multipart/form-data" action="/mainpage/editimage" method="POST" id="imageuploadform">
                            <input type="file" name="settingsimagename" id="imagename">
                            {{!-- <input type="submit" name="submit" value="" id="editimagesubmit"> --}}
                        </form>
                    </div>
                    <div class="settingslines">
                        <h1 class="settingstitles">Username:</h1>
                    </div>
                    <div class="settingslines">
                        <h1 id="settingsusername">{{user.name}}</h1>
                    </div>
                    <div class="settingslines">
                        <input type="text" name="" id="settingsusernameinput" class="settinginputs">
                        <button class="settingsbtns" id="editusername"><i class="far fa-edit"></i></button>
                    </div>
                    <div class="settingslines">
                        <h1 class="settingstitles">Email:</h1>
                    </div>
                    <div class="settingslines">
                        <p id="settingsemail">{{user.email}}</p>
                    </div>
                    <div class="settingslines">
                        <input type="text" name="" id="settingsemailinput" class="settinginputs">
                        <button class="settingsbtns" id="editemail"><i class="far fa-edit"></i></button>
                    </div>
                    <div class="settingslines">
                        <h1 class="settingstitles">Password:</h1>
                    </div>
                    <div class="settingslines">
                        <p id="msgsettingpsw"></p>
                    </div>
                    <div class="settingslines">
                        <input type="text" name="" id="settingspasswordinput" class="settinginputs">
                        <button class="settingsbtns" id="editpassword"><i class="far fa-edit"></i></button>
                    </div>

                </div>
            </div>
        </div>
    </div>
    <div class="modal" tabindex="-1" role="dialog" id="modalhuntingtools">
        <div class="modal-dialog" role="document">
            <div class="modal-content">
                <div class="modal-header">
                    <h5 class="modal-title">Hunting tools</h5>
                    <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                        <span aria-hidden="true">&times;</span>
                    </button>
                </div>
                <div class="modal-body">
                    <div class="huntingtoolslines">
                        <h1 class="huntingtoolstitles"><i class="fab fa-linkedin"></i> Linkedin</h1>
                    </div>
                    <div class="huntingtoolslines">
                        <h1 class="huntingtools">linkedin/abm</h1>
                    </div>
                    <div class="huntingtoolslines">
                        <input type="text" name="" id="" class="huntingtoolsinputs">
                        <button class="huntingtoolsbtns"><i class="far fa-edit"></i></button>
                    </div>
                    <div class="huntingtoolslines">
                        <h1 class="huntingtoolstitles"><i class="fab fa-github-square"></i> Github</h1>
                    </div>
                    <div class="huntingtoolslines">
                        <h1 class="huntingtools">github/abm</h1>
                    </div>
                    <div class="huntingtoolslines">
                        <input type="text" name="" id="" class="huntingtoolsinputs">
                        <button class="huntingtoolsbtns"><i class="far fa-edit"></i></button>
                    </div>
                    <div class="huntingtoolslines">
                        <h1 class="huntingtoolstitles"><i class="far fa-id-card"></i> Portfolio</h1>
                    </div>
                    <div class="huntingtoolslines">
                        <h1 class="huntingtools">portfolio/abm</h1>
                    </div>
                    <div class="huntingtoolslines">
                        <input type="text" name="" id="" class="huntingtoolsinputs">
                        <button class="huntingtoolsbtns"><i class="far fa-edit"></i></button>
                    </div>
                    <div class="huntingtoolslines">
                        <h1 class="huntingtoolstitles"><i class="far fa-file-alt"></i> Resume</h1>
                    </div>
                    <div class="huntingtoolslines">
                        <h1 class="huntingtools">resume/abm</h1>
                    </div>
                    <div class="huntingtoolslines">
                        <input type="text" name="" id="" class="huntingtoolsinputs">
                        <button class="huntingtoolsbtns"><i class="far fa-edit"></i></button>
                    </div>

                </div>
            </div>
        </div>
    </div>

    <script src="https://code.jquery.com/jquery.js"></script>
    <script src="https://ajax.googleapis.com/ajax/libs/jqueryui/1.11.4/jquery-ui.min.js"
        type="text/javascript"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"
        integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1"
        crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
        integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM"
        crossorigin="anonymous"></script>
    <script src="http://malsup.github.com/jquery.form.js"></script> 
    <script src="https://kit.fontawesome.com/fe613f4734.js"></script>
    <script src="/js/mainpage.js"></script>
</body>