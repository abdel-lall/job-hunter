<head>
    <meta charset="utf-8">

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0-beta.3/css/bootstrap.min.css">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/css/bootstrap.min.css"
        integrity="sha384-ggOyR0iXCbMQv3Xipma34MD+dH/1fQ784/j6cY/iJTQUOhcWr7x9JvoRxT2MZw1T" crossorigin="anonymous">
    <link rel="stylesheet" href="/styles/dashboard.css">
    <title>Dashboard</title>
</head>

<body>
    <nav class="navbar navbar-dark flex-md-nowrap " id="navbar">
        <a class="navbar-brand col-md-2 mr-0" href="#" id="titleJ"><span id="jobS">Job</span><span
                id="listJ">Hunter</span></a>
        <ul class="nav nav-pills nav-fill" id="menuMobileSaved">
            <li class="nav-item">
                <a class="nav-link menuItem" href="/dashboard" id="jobSearchM" style=>
                    Search for jobs
                </a>
                <a class="nav-link menuItem" href="/saved" id="jobListM">
                    <span class="nameUser">{{name}}</span> job list
                </a>
            </li>
          
        </ul>

        <ul class="navbar-nav px-3">
            <li class="nav-item text-nowrap">
                <button type="button" class="btn btn-outline-primary" id="logout">Sign out</button>
            </li>
        </ul>
    </nav>

    <div class="container-fluid" id="containerMain">
        <div class="row" id="contentRow">
            <nav class="col-md-2 d-none d-md-block sidebar" id="sidebar">
                <div class="sidebar-sticky">
                    <ul class="nav flex-column">
                        <li class="nav-item" id="helloDiv">
                            <a class="nav-link active" href="#">

                                <p id="hello">Hello <span class="nameUser">{{data.name}}</span></p>
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link menuItem" href="/dashboard" id="jobSearch">
                                Search for jobs
                            </a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link menuItem" href="/saved" id="jobList">
                                <span class="nameUser">{{data.name}}</span> job list
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>
            <main role="main" class="col-md-9 col-lg-10 px-4" id="main">
                <div class="container-fluid" id="resSection">
                    {{#each data}}
                    <div class="row cardRow">
                        <div class="col-lg">
                            <div class="card" data-id={{dataValues.id}}>
                                <div class="card-body">
                                    <h5 class="card-title">{{dataValues.title}}</h5>
                                    <p class="card-text employer">{{dataValues.employer}}</p>
                                    <p class="card-text location">{{dataValues.location}}</p>
                                    <p class="card-text">Description :
                                        {{dataValues.description}}</p>
                                    <a href="#" class="btn btn-outline-primary delete"
                                        data-id={{dataValues.id}}>Delete</a>
                                    <a href={{dataValues.url}} class="btn btn-outline-primary"
                                        target="_blank">Apply</a>
                                </div>
                            </div>
                        </div>
                    </div>
                    {{/each}}
                </div>
        </div>
        </main>
    </div>
    </div>
    <div class="modal" tabindex="-1" role="dialog" id="modal">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <h5 class="modal-title"><span id="jobSModal">Job</span><span
                id="listJModal">Hunter</span></h5>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        <p id="msgModal"></p>
      </div>
      <div class="modal-footer">
        
        <button type="button" class="btn btn-outline-primary" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>
    <script src="https://code.jquery.com/jquery.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.7/umd/popper.min.js"
        integrity="sha384-UO2eT0CpHqdSJQ6hJty5KVphtPhzWj9WO1clHTMGa3JDZwrnQq4sF86dIHNDz0W1"
        crossorigin="anonymous"></script>
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.3.1/js/bootstrap.min.js"
        integrity="sha384-JjSmVgyd0p3pXB1rRibZUAYoIIy6OrQ6VrjIEaFf/nJGzIxFDsf4x0xIM+B07jRM"
        crossorigin="anonymous"></script>
    <script src="https://kit.fontawesome.com/fe613f4734.js"></script>
    <script src="/js/saved.js"></script>
</body>