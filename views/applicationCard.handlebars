<div class="applicationCard" data-id={{data.id}}>
    <a href={{data.url}} class="ApositinTitle">{{data.title}}</a>
    <div class="AcompanyName"><i class="far fa-building buidingIcon"></i>{{data.organisation}}</div>
    <div class="ApositionLocation"><i class="fas fa-map-marker-alt mapmarkerIcon"></i>{{data.location}}</div>
    <div class="ApositionDescription">
        {{data.description}}
    </div>
    <button class="Anext" data-id={{data.id}}><i class="fas fa-arrow-right"></i></button>
</div>

                                       